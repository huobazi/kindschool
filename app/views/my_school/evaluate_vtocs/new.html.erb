<div class="show_work">
  <%= render :partial => "/modules/#{view_layout}/work_menu" %>
  <div class="work">
    <div class="headline">
      <div class="headline_title">
        <div class="messages_root"></div>
        <label>评估管理
        </label>
        <div class="crumbs">
          <a href="/my_school/evaluates" title="返回首页">
            <div class="icon_home"></div>
          </a>
          <ul>
             <li><%= link_to "评估管理", my_school_evaluates_path %></li>
            <li>/</li>
            <li><%=link_to @evaluate_entry.name ,my_school_evaluate_evaluate_entry_path(@evaluate_entry.evaluate.id,@evaluate_entry.id)%></li>
            <li>/</li>
            <li>添加资料目录管理</li>
          </ul>
        </div>
      </div>
    </div>

    <div class="work_form">
      <div class="form_div">
        <%= form_for(@evaluate_vtoc,:url=>my_school_evaluate_vtocs_path(:evaluate_entry_id=>@evaluate_entry.id),:html =>{:multipart=>true,:class=>"form-horizontal"}) do |f| %>
          <%= render 'form', :f=>f %>
        <%end%>
        <hr/>          
      </div>
    </div>
  </div>

</div>