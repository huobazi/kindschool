<script type="text/javascript">
  $(function() {
    $("#dialog_change_users" ).dialog({
      dialogClass: "no-close",
      autoOpen: false,
      height:500,
      width:700,
      buttons: {
        "确定": function () {
          $(this).dialog("close");
        },
        "取消": function () {
          $("#message_inputs").html("");
          $(this).dialog("close");
        }
      }
    });
    $( "#change_users" ).click(function() {
      $( "#dialog_change_users" ).dialog("open");
    });
  });
</script>
<div class="show_work">
  <%= render :partial => "/modules/colorful/work_menu" %>
  <div class="work">
    <div class="headline">
      <div class="headline_title">
        <div class="messages_root"></div>
        <label>审核设置</label>
        <div class="crumbs">
          <a href="/my_school/home" title="返回首页">
            <div class="icon_home"></div>
          </a>
          <ul>
          </ul>
        </div>
      </div>
      <ul class="work_action">
         <li>
        </li>
        <li>
          <a href="/" title="删除" id="destroy_multiple">
