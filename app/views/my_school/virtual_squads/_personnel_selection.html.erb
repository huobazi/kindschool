<div class="control-group">
  <label class="control-label">人员组成</label>
  <div class="controls">
    <label class="show_label">
      <%= link_to "点击选择","###" ,:id=>"change_users"%>
    </label>
  </div>
</div>
<div class="control-group">
  <label class="control-label">已选名单</label>
  <div class="controls">
    <label class="show_label">
      <div id="message_inputs">

      </div>
    </label>
  </div>
</div>
<script type="text/javascript">
  $(function() {
<% if action_name == "edit"%>
      $(window).load(function(){
        $("#message_inputs").load("<%= url_for(:controller=>'/my_school/virtual_squads',:action=>:get_edit_ids,:id=>@virtual_squad.id) %>");
      })
<% end %>
  });
</script>


