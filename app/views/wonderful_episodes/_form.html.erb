<%= simple_form_for(@wonderful_episode) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :kindergarten_id %>
    <%= f.input :squad_id %>
    <%= f.input :url_address %>
    <%= f.input :is_top %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
